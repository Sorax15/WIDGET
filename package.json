{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "watch:ford": "mix",
        "dev:ford": "npm run development --theme=ford",
        "dev:toyota": "npm run development --theme=toyota",
        "prod:ford": "npm run production --theme=ford",
        "prod:toyota": "npm run production --theme=toyota"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.12",
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.4.18",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.55.0",
        "sass-loader": "^12.1.0",
        "tailwindcss": "^3.1.8"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^6.2.0",
        "node-sass": "^6.0.1",
        "postcss-import": "^15.0.0"
    }
}
